#include <stdio.h>

int hanoi(int origem, int auxiliar, int destino){
  if(origem == 0) return 0;
  
  auxiliar = origem-(origem-1);
  printf("Auxiliar: %d ", auxiliar);
  destino = origem-(origem-1);
  printf("Destino: %d ", destino);
  origem -= 2;
  printf("Origem: %d ", origem);
  destino += auxiliar;
  printf("Destino: %d\n", destino);
  
    
  return hanoi(origem, auxiliar, destino);
 
  
  
}

int main(void){
  int ori, aux = 0, des = 0;
  scanf("%d", &ori);
  
  hanoi(ori, aux, des);
  
  
  
  
  
  return 0;
}